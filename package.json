{
  "name": "goit-js-hw-08-gallery",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "parcel index.html",
    "build": "parcel build index.html --public-url /goit-js-hw-08-gallery/",
    "deploy": "gh-pages -d dist",
    "predeploy": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SerhiyStepanov/goit-js-hw-08-gallery.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SerhiyStepanov/goit-js-hw-08-gallery/issues"
  },
  "homepage": "https://SerhiyStepanov.github.com/goit-js-hw-08-gallery",
  "devDependencies": {
    "parcel-bundler": "^1.12.4"
  },
  "dependencies": {
    "gh-pages": "^3.1.0",
    "parcel-plugin-custom-dist-structure": "^1.1.16",
    "parcel-plugin-nuke-dist": "^1.0.1"
  },
  "customDistStructure": {
  "config": {
    ".js": "js",
    ".html":"html",
    ".css":"css",
    "images": [
      ".jpg",
      ".png",
      ".svg"
    ],
    ".fileExtension": "folder/in/dist",
    "folder/in/dist": [ ".file", ".extensions" ]
  },
  "options": {
    "development": true
  }
}
}
